<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lightcontroller.TimeoutController">
   <children>
      <Label layoutX="39.0" layoutY="14.0" text="Serial connection timed out" AnchorPane.leftAnchor="15.0" AnchorPane.topAnchor="15.0">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label layoutX="39.0" layoutY="90.0" prefHeight="53.0" prefWidth="207.0" text="Make sure Arduino is connected and serial monitor is off." wrapText="true" AnchorPane.leftAnchor="15.0" AnchorPane.topAnchor="50.0" />
      <Button fx:id="closeButton" layoutX="15.0" layoutY="136.0" mnemonicParsing="false" onMouseClicked="#onCloseButtonClicked" text="Close" AnchorPane.leftAnchor="15.0" AnchorPane.topAnchor="130.0" />
      <Button fx:id="retryButton" layoutX="75.0" layoutY="130.0" mnemonicParsing="false" onMouseClicked="#onRetryButtonClicked" text="Retry" AnchorPane.topAnchor="130.0" />
      <Label fx:id="retryLabel" layoutX="136.0" layoutY="134.0" text="Retrying..." visible="false" />
   </children>
</AnchorPane>
